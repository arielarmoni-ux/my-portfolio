\f0\fs24 \cf0 :root \{ \
    --bg: #fff; \
    --main: #000; \
    --muted: #888; \
    --nav-width: 320px; \
    --grid-gap: 35px; /* \uc0\u1502 \u1512 \u1493 \u1493 \u1495  \u1513 \u1493 \u1493 \u1492  \u1493 \u1502 \u1488 \u1493 \u1494 \u1503  */\
\}\
\
* \{ box-sizing: border-box; \}\
\
html, body \{ \
    margin: 0; \
    padding: 0; \
    font-family: 'Inter', sans-serif; \
    background: var(--bg); \
    overflow-x: hidden; \
    -webkit-font-smoothing: antialiased; \
\}\
\
/* --- \uc0\u1502 \u1489 \u1504 \u1492  \u1499 \u1500 \u1500 \u1497  \u1493 \u1514 \u1504 \u1493 \u1506 \u1514  \u1505 \u1497 \u1497 \u1491 \u1489 \u1512  --- */\
#site-wrapper \{ \
    position: relative; \
    width: 100%; \
    margin: 0; \
    transition: all 0.9s cubic-bezier(0.23, 1, 0.32, 1); \
    background: var(--bg); \
    min-height: 100vh; \
    z-index: 1000; \
\}\
\
body.nav-open #site-wrapper \{ \
    margin-left: var(--nav-width); \
    width: calc(100% - var(--nav-width)); \
\}\
\
#side-nav \{ \
    position: fixed; \
    top: 0; \
    left: calc(-1 * var(--nav-width)); \
    width: var(--nav-width); \
    height: 100vh; \
    background: var(--bg); \
    z-index: 3000; \
    padding: 40px 30px; \
    overflow-y: auto; \
    transition: left 0.9s cubic-bezier(0.23, 1, 0.32, 1); \
    text-align: left; \
    border-right: 1px solid #f0f0f0; \
    direction: ltr; \
\}\
\
body.nav-open #side-nav \{ left: 0; \}\
\
/* --- \uc0\u1496 \u1497 \u1508 \u1493 \u1490 \u1512 \u1508 \u1497 \u1492  \u1505 \u1497 \u1497 \u1491 \u1489 \u1512  \u1506 \u1501  \u1492 \u1493 \u1489 \u1512  --- */\
.nav-links a \{ \
    font-size: 14px; \
    text-decoration: none; \
    color: var(--main); \
    text-transform: uppercase; \
    letter-spacing: 1.5px; \
    font-weight: 600; \
    display: block; \
    margin-bottom: 12px; \
    transition: 0.3s;\
\}\
\
.nav-links a:hover \{ \
    color: var(--muted); \
    transform: translateX(4px); \
\}\
\
.project-index-title \{ \
    font-size: 11px; \
    text-transform: uppercase; \
    color: var(--muted); \
    margin-bottom: 15px; \
    border-top: 1px solid #eee; \
    padding-top: 20px; \
    letter-spacing: 2px; \
\}\
\
.project-list-nav a \{ \
    font-size: 15px; \
    text-decoration: none; \
    color: var(--main); \
    display: block; \
    margin-bottom: 10px; \
    font-weight: 400; \
    line-height: 1.2; \
    transition: 0.3s; \
\}\
\
.project-list-nav a:hover \{ \
    color: var(--muted);\
    transform: translateX(8px); \
\}\
\
/* --- Header & Logo --- */\
header \{ \
    position: relative; \
    min-height: 100px; \
    padding: 40px 5% 20px; \
    display: flex; \
    flex-direction: column; \
    justify-content: center; \
    align-items: center; \
    border-bottom: 1px solid #f0f0f0; \
\}\
\
.logo \{ \
    font-size: 29px; \
    letter-spacing: 5px; \
    text-transform: uppercase; \
    text-decoration: none; \
    color: #000; \
\}\
\
.nav-wrapper \{ \
    position: absolute; \
    left: 5%; \
    top: 52px; \
    z-index: 4000; \
\}\
\
/* --- \uc0\u1500 \u1493 \u1490 \u1497 \u1511 \u1514  \u1492 \u1488 \u1497 \u1511 \u1505  (X) --- */\
.menu-toggle \{ \
    display: flex; \
    flex-direction: column; \
    justify-content: space-between; \
    width: 26px; \
    height: 14px; \
    cursor: pointer; \
    border: none; \
    background: none; \
    padding: 0; \
\}\
\
.menu-toggle span \{ \
    display: block; \
    width: 100%; \
    height: 1px; \
    background: var(--main); \
    transition: 0.3s cubic-bezier(0.23, 1, 0.32, 1); \
\}\
\
body.nav-open .menu-toggle span:nth-child(1) \{ transform: translateY(6.5px) rotate(45deg); \}\
body.nav-open .menu-toggle span:nth-child(2) \{ opacity: 0; \}\
body.nav-open .menu-toggle span:nth-child(3) \{ transform: translateY(-6.5px) rotate(-45deg); \}\
\
/* --- \uc0\u1491 \u1507  \u1492 \u1489 \u1497 \u1514 : \u1490 \u1512 \u1497 \u1491  \u1512 \u1497 \u1489 \u1493 \u1506 \u1497  \u1493 \u1514 \u1493 \u1499 \u1503  \u1500 \u1513 \u1502 \u1488 \u1500  --- */\
.portfolio-grid \{ \
    display: grid; \
    grid-template-columns: repeat(5, 1fr); \
    gap: var(--grid-gap); \
    padding: 60px 5% 100px; \
    max-width: 1800px; \
    margin: 0 auto; \
\}\
\
.project-card \{ \
    display: flex; \
    flex-direction: column; \
    text-decoration: none; \
    transition: transform 0.7s cubic-bezier(0.23, 1, 0.32, 1); \
    position: relative; \
    z-index: 1; \
\}\
\
.project-card:hover \{ \
    transform: scale(1.06); \
    z-index: 10; \
\}\
\
.img-container \{ \
    width: 100%; \
    aspect-ratio: 4/3; \
    overflow: hidden; \
    background: #f7f7f7; \
\}\
\
.img-container img \{ \
    width: 100%; \
    height: 100%; \
    object-fit: cover; \
    display: block; \
\}\
\
.project-info \{ \
    padding: 12px 0; \
    opacity: 0; \
    transition: opacity 0.4s ease; \
    text-align: left; /* \uc0\u1497 \u1497 \u1513 \u1493 \u1512  \u1500 \u1513 \u1502 \u1488 \u1500  */\
    direction: ltr;    /* \uc0\u1499 \u1497 \u1493 \u1493 \u1503  \u1496 \u1511 \u1505 \u1496  \u1488 \u1504 \u1490 \u1500 \u1497 /\u1489 \u1497 \u1504 \u1500 \u1488 \u1493 \u1502 \u1497  */\
\}\
\
.project-card:hover .project-info \{ opacity: 1; \}\
\
.project-title \{ \
    font-size: 11px; \
    letter-spacing: 1.2px; \
    text-transform: uppercase; \
    color: var(--main); \
    font-weight: 500; \
    line-height: 1.2; \
\}\
\
/* --- \uc0\u1491 \u1507  \u1508 \u1512 \u1493 \u1497 \u1511 \u1496  --- */\
.project-meta \{ max-width: 1200px; margin: 30px auto 20px; padding: 0 5%; display: flex; flex-direction: column; gap: 5px; direction: ltr; text-align: left; \}\
.meta-item \{ line-height: 1.3; \}\
.meta-item .label \{ font-weight: 400; color: var(--muted); margin-right: 8px; \}\
.meta-item .value \{ font-weight: 600; color: var(--main); \}\
.project-description \{ max-width: 1200px; margin: 0 auto 50px; padding: 0 5%; font-size: 18px; line-height: 1.55; text-align: left; direction: ltr; white-space: pre-wrap; \}\
.gallery \{ max-width: 1200px; margin: 0 auto; padding: 0 5%; \}\
.gallery img \{ width: 100%; height: auto; margin-bottom: 40px; display: block; \}\
\
.nav-arrow \{ position: fixed; top: 50%; transform: translateY(-50%); z-index: 2500; text-decoration: none; color: var(--main); font-size: 30px; opacity: 0.2; padding: 20px; \}\
\
/* --- \uc0\u1492 \u1514 \u1488 \u1502 \u1493 \u1514  \u1502 \u1493 \u1489 \u1497 \u1497 \u1500  --- */\
@media (max-width: 768px) \{\
    header \{ padding-top: 75px; min-height: 120px; \}\
    .nav-wrapper \{ position: fixed; left: 20px; top: 22px; z-index: 5000; \}\
    .project-description \{ line-height: 1.5; font-size: 16px; text-align: center; \}\
    .project-meta \{ align-items: center; text-align: center; margin-top: 20px; gap: 4px; \}\
    .portfolio-grid \{ \
        grid-template-columns: repeat(2, 1fr); \
        gap: 20px; \
    \}\
    .nav-arrow \{ display: none; \}\
    body.nav-open #site-wrapper \{ transform: translateX(var(--nav-width)); margin-left: 0; width: 100%; \}\
\}}